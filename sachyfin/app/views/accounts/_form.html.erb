<%= simple_form_for(@account) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :account_number %>
    <%= f.input :name %>
    <%= f.input :last_import %>
    <%= f.input :last_transaction %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
